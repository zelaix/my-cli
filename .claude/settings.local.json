{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(mv:*)",
      "Bash(my-cli:*)",
      "Bash(python:*)",
      "Bash(MY_CLI_API_KEY=\"fake-key-for-testing\" python -m my_cli chat --model gemini-2.0-flash \"what does this project do?\")",
      "Bash(MY_CLI_DEBUG=true my-cli chat --model gemini-2.0-flash-exp \"List the files in the current directory\")",
      "Bash(MY_CLI_LOG_LEVEL=DEBUG my-cli chat --model gemini-2.0-flash-exp \"List the files in the current directory\")",
      "Bash(export MY_CLI_LOG_LEVEL=DEBUG)",
      "Bash(timeout:*)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY timeout 10s python debug_simple_chat.py)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python debug_function_response.py)",
      "Bash(touch:*)",
      "Bash(MY_CLI_LOG_LEVEL=DEBUG my-cli chat --tools --model gemini-2.0-flash-exp \"List the files in the current directory\")",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python test_function_calling.py)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python test_multi_step_function_calling.py)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python debug_conversation_history.py)",
      "Bash(export MY_CLI_API_KEY=$MY_CLI_API_KEY)",
      "WebFetch(domain:platform.moonshot.ai)",
      "WebFetch(domain:github.com)",
      "Bash(find:*)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python debug_tool_count.py)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python debug_cli_flow.py)",
      "Bash(MY_CLI_API_KEY=$MY_CLI_API_KEY python debug_kimi_streaming.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY python debug_kimi_streaming.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY python debug_cli_flow.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY timeout 10s my-cli chat --model kimi-k2-instruct \"list the current directory\")",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY timeout 15s my-cli chat --model kimi-k2-instruct \"list the current directory\")",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY python debug_id_flow.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY timeout 10s python debug_cli_flow.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY python debug_tool_schema.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY python debug_raw_streaming.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY python debug_non_streaming.py)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY timeout 15s my-cli chat --model kimi-k2-base \"list the current directory\")",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY timeout 15s my-cli chat --model kimi-k2-base \"hello\" --debug)",
      "Bash(MY_CLI_KIMI_API_KEY=$MY_CLI_KIMI_API_KEY timeout 15s my-cli chat --model kimi-k2-base \"hello\")"
    ],
    "deny": []
  }
}